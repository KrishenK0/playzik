{
  "name": "playzik",
  "productName": "playzik",
  "version": "1.0.0",
  "description": "Participating playlist electron app",
  "main": "index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "start-server": "nodemon server.js",
    "dev": "concurrently --kill-others \"npm run start:tailwind\" \"npm start\"",
    "start:tailwind": "npx postcss ./views/css/tailwind.css -o ./public/css/style.css --watch",
    "production:tailwind": "cross-env NODE_ENV=production postcss ./views/css/tailwind.css -o ./public/css/style.css"
  },
  "keywords": [],
  "author": {
    "name": "KrishenK",
    "email": "fox2581kriskenk@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "playzik"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "googleapis": "^101.0.0",
    "https": "^1.0.0",
    "mariadb": "^3.0.1",
    "nodemon": "^2.0.16",
    "socket.io": "^4.5.1",
    "socket.io-stream": "^0.9.1",
    "stream": "^0.0.2",
    "superagent": "^7.1.6",
    "tmp": "^0.2.1",
    "ws": "^8.7.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "daisyui": "^2.15.2",
    "electron": "19.0.1",
    "electron-reloader": "^1.2.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "ytdl-core": "^4.11.2"
  }
}
